          <label for="price">Price</label>
          <input type="text" name="price" value="<%=tableTop.price%>">
          <label for="qty">Quantity</label>
          <input type="text" name="qty" value="<%=tableTop.qty%>">
        </fieldset>
        <div class="buttons">
        <input class="btn btn-sm btn-outline-secondary" type="submit" value="Submit Changes">
      </form>
      <form action="/tableTop/<%=tableTop.id%>?_method=DELETE" method="POST">
        <input class="btn btn-sm btn-outline-secondary" type="submit" value="DELETE">
      </form>
