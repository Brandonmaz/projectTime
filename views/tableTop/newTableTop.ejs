  </header>


  <div class="edit-container">
    <div class="edit-sidebar"></div>
    <div class="edit-page">


        <fieldset class="edit-info">
          <legend><h1 class="name"><%= currentUser.username%>,</h1>
            <h2>Create a New Table Top Game</h2>
          </legend>
      <form action="/tableTop" method="POST">
        <label for="name">Name</label>
        <input type="text" name="name">
        <label for="img">Image</label>
        <input type="text" name="img">
        <label for="img">Image Carousel</label>
        <input type="text" name="img1">
        <label for="img">Image Carousel</label>
        <input type="text" name="img2">
        <label for="img">Image Carousel</label>
        <input type="text" name="img3">
        <label for="players">Players</label>
        <input type="text" name="players">
        <label for="price">Price</label>
        <input type="text" name="price">
        <label for="qty">Quantity</label>
        <input type="text" name="qty">
      </fieldset>
      <div class="buttons">
        <input class="btn btn-sm btn-outline-secondary" type="submit" value="Create Product">
      </div>
      <fieldset class="edit-info">
        <label for="description">Description</label>
        <textarea id="description" type="text" name="description"></textarea>
      </fieldset>
      </form>
      <button class="btn btn-sm btn-outline-secondary" type="button" name="button">
        <a class="a-button" href="/tableTop">Back to Tabletop Simulator</a>
      </button>
    </div>
  </div>
  <footer class="container">
    <p class="float-end"><a href="#">Back to top</a></p>
