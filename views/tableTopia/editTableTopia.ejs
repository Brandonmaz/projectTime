<body>
  <header>
    <nav class="navbar navbar-expand-md navbar-dark fixed-top">
      <div class="container-fluid">
        <a class="navbar-brand" href="#">Life</a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarCollapse">
          <ul class="navbar-nav me-auto mb-2 mb-md-0">
            <li class="nav-item active">
              <a class="nav-link" aria-current="page" href="/products">Home</a>
            </li>
              <% if(currentUser){ %>
                <!-- <h4>Welcome, <%= currentUser.username%></h4> -->
                <div class="collapse navbar-collapse" id="navbarNavDarkDropdown">
                  <ul class="navbar-nav">
                    <li class="nav-item dropdown">
                      <a class="nav-link dropdown-toggle" id="navbarDarkDropdownMenuLink" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                        Dropdown
                      </a>
                      <ul class="dropdown-menu dropdown-menu-dark" aria-labelledby="navbarDarkDropdownMenuLink">
                        <li><a class="dropdown-item" href="/tableTop/newTableTop">New Tabletopia</a></li>
                        <li><a class="dropdown-item" href="/tableTop/seed/newProducts">Seed Tabletopia</a></li>
                      </ul>
                    </li>
                  </ul>
                  <form action="/sessions<%=tableTopia.id%>?_method=DELETE" method="post">
                    <input class="logOut" type="submit" name="" value="Log Out">
                  </form>
                </div>
              <%}else{%>
                <li class="nav-item">
                  <a class="nav-link" href="/users/new">SignUp</a>
                </li>
                <li class="nav-item">
                  <a class="nav-link" href="/sessions/new">LogIn</a>
                </li>
              <%}%>
          </ul>
          <form class="d-flex">
            <% if(currentUser){%>
            <input class="form-control me-2" type="search" placeholder="Welcome <%= currentUser.username%>" aria-label="Search">
            <%}else{%>
              <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
              <%}%>
            <button class="btn btn-outline-success" type="submit">Search</button>
          </form>
        </div>
      </div>
    </nav>
  </header>


  <div class="edit-container">
    <div class="edit-sidebar"></div>
    <div class="edit-page">

      <form action="/tableTopia/<%=tableTopia.id%>?_method=PUT" method="POST">
        <fieldset class="edit-info">
          <legend>
            <h2>Edit <%=tableTopia.name%></h2>
          </legend>
          <label for="name">Name</label>
          <input type="text" name="name" value="<%=tableTopia.name%>">
          <label for="img">Image</label>
          <input type="text" name="img" value="<%=tableTopia.img%>">
          <label for="players">Players</label>
          <input type="text" name="players" value="<%=tableTopia.players%>">
          <label for="description">Description</label>
          <input type="text" name="description" value="<%=tableTopia.description%>">
          <label for="price">Price</label>
          <input type="text" name="price" value="<%=tableTopia.price%>">
          <label for="qty">Quantity</label>
          <input type="text" name="qty" value="<%=tableTopia.qty%>">
        </fieldset>
        <input class="btn btn-sm btn-outline-secondary" type="submit" value="Submit Changes">


      </form>
      <form action="/tableTopia/<%=tableTopia.id%>?_method=DELETE" method="POST">
        <input class="btn btn-sm btn-outline-secondary" type="submit" value="DELETE">
      </form>
      <fieldset class="edit-info">
        <legend>
          <h2>Condition of Item</h2>
        </legend>
        <label for="condition">Description</label>
        <textarea id="condition"type="text" name="condition" placeholder="<%=tableTopia.condition%>"></textarea>
      </fieldset>
      <button class="btn btn-sm btn-outline-secondary" type="button" name="button">
        <a class="a-button" href="/tableTopia">Back to Tabletopia</a>
      </button>
    </div>
  </div>
</body>
</html>
